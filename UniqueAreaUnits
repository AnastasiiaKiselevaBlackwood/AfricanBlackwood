import pandas
realty_df = pandas.read_csv('yandex_realty_data.csv')

unique_area_units = []

for value in realty_df['area_unit']:
    if value not in unique_area_units:
        unique_area_units.append(value) # найдите уникальные значения столбца 'area_unit'
 # и поместите их в список unique_area_units

print(unique_area_units)
